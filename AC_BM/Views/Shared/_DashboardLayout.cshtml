<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- boostrap cdn -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <!-- Fontawesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css" integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g==" crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="~/css/style.css">
    <link rel="stylesheet" href="~/css/site.css">
    <!-- Remixicons -->
    <link href="https://cdn.jsdelivr.net/npm/remixicon@2.5.0/fonts/remixicon.css" rel="stylesheet">
    <!-- page css -->
    <link rel="stylesheet" href="~/css/DashboardStyleSheet.css" asp-append-version="true">

    <!-- Bootsrap -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/fomantic-ui/2.8.8/semantic.min.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/1.12.0/css/dataTables.semanticui.min.css">
    <link rel="stylesheet" href="https://cdn.datatables.net/1.12.0/css/jquery.dataTables.min.css">

    <!-- Toaster -->
    <link href="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdn.datatables.net/1.12.1/css/jquery.dataTables.min.css" />
    <link rel="stylesheet" href="https://cdn.datatables.net/buttons/2.2.3/css/buttons.dataTables.min.css" />
    <!-- chart.js cdn -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.4/Chart.js"></script>
    <title>AC IMMIGRATION</title>
</head>
<body>
    <div class="top-navigation">
        <div class="container-fluid">
            <div class="logos">
                <img class="Algoz-logo" src="~/Images/AC_Logo.jpg" alt="logo" srcset="">
            </div>
            <div class="right-navbar">
                <div class="profile-items">
                    <div class="widgets">
                        <a>
                            <i class="fa-solid fa-gear me-3 fa-lg widgets-icon text-primary-2"></i>
                        </a>
                    </div>
                    <div class="profile-img">
                        <img onclick="showMenu()" class="profile-avatar rounded-circle" src="~/Images/Corporate/book_pen.jpg" alt="profile">
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!--Profil e dropdown starts here-->

    <div class="Profile-dropdown" id="show-dropdown">
        <div class="details">
            <a class="profile mb-2 text-primary-2">
                <span class="fa-solid fa-user mb-2"></span> Profile
            </a>
            <form method="post">
                <button type="submit" class="btn btn-link text-primary-2"><i class="fas fa-sign-out-alt"></i> Logout</button>
            </form>
        </div>
    </div>

    <!--Profile dropdown ends here-->

    <div class="pagebody-wrapper">
        <!--Start of Side Navigation -->
        <div class="Side-navigation primary-color" id="side-nav">
            <ul>
                <li class="list active" id="dashboard">
                    <a>
                        <span class="icon"><ion-icon name="home-outline"></ion-icon></span>
                        <span class="title">Dashboard</span>
                    </a>
                </li>

                <li class="list" id="Companies">
                    <a asp-action="Add" asp-controller="Client">
>
                        <span class="icon"><ion-icon name="documents-outline"></ion-icon></span>
                        <span class="title">AddUsers</span>
                    </a>
                </li>

                <li class="list" id="Users">
@*                    <a asp-action="Users" asp-controller="Admin">
*@
                 <a asp-action="ClientList" asp-controller="Client">
                        <span class="icon"><ion-icon name="documents-outline"></ion-icon></span>
                        <span class="title">Users</span>
                    </a>
                </li>  
              
                <li class="list" id="Reporting">
                    <a asp-action="Edit" asp-controller="Client">
                        <span class="icon"><ion-icon name="clipboard-outline"></ion-icon></span>
                        <span class="title">UpdateUser</span>
                    </a>
                </li>
               
                        <li class="list mt-4" id="Settings">
                            <a  asp-action="Logout"  asp-controller="UserAunthentication">
                        <span class="icon"><ion-icon name="power-outline"></ion-icon></span>
                                <span class="title">Logout</span>
                            </a>
                        </li>

            </ul>
        </div>
        <!--End of Side Navigation -->
        <!--Start Inner page Content -->

        <div class="Innerwrapper-section">
            <div class="page">
                <div class="inner-page">
                    <div class="page-title">
                        <h2 class="text-primary-2">@ViewData["Title"]</h2>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            @RenderBody()
                        </div>
                    </div>

                </div>
            </div>
        </div>
        <!--End Inner page Content -->
    </div>
    <script src="https://code.jquery.com/jquery-3.5.1.js"></script>
    <!--Datatalble -->
    <script src="https://cdn.datatables.net/1.12.0/js/jquery.dataTables.min.js"></script>
    <script src="https://cdn.datatables.net/1.12.0/js/dataTables.semanticui.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/fomantic-ui/2.8.8/semantic.min.js"></script>
    <script src="https://unpkg.com/js-alert/dist/jsalert.min.js"></script>


    @*my scripts starts here*@
    <!-- bootsrap js -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
    <script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
    <script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>
    <script src="~/js/Style.js" asp-append-version="true"></script>

  <!-- Alpine Plugins -->
<script defer src="https://unpkg.com/%40alpinejs/persist@3.x.x/dist/cdn.min.js"></script>
<!-- Alpine Core -->
<script defer src="https://unpkg.com/alpinejs@3.x.x/dist/cdn.min.js"></script>

    <!-- Alpine Plugins -->
    <script defer src="https://unpkg.com/%40alpinejs/persist@3.x.x/dist/cdn.min.js"></script>

    <!-- Alpine Core -->
    <script defer src="https://unpkg.com/alpinejs@3.x.x/dist/cdn.min.js"></script>

    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.2.3/js/dataTables.buttons.min.js"></script>
    <partial name="_ValidationScriptsPartial" />
    @RenderSection("Scripts",required:false)
</body>
</html>
