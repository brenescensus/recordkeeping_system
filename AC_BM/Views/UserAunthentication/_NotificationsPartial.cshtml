@using AC_BM.Models.Data;
@if (TempData[Constants.Success] != null)
{
    <script>
        toastr.options = {
            "closeButton": true,
            "progressBar": true,
        }
        toastr.success("@TempData[Constants.Success]");

    </script>

}
@if (TempData[Constants.Error] != null)
{
    <script>
        toastr.options = {
            "closeButton": true,
            "progressBar": true,
        }
        toastr.error("@TempData[Constants.Error]");

    </script>
}
@if (TempData[Constants.Warning] != null)
{
    <script>
        toastr.options = {
            "closeButton": true,
            "progressBar": true,
        }
        toastr.warning("@TempData[Constants.Warning]");

    </script>
}

